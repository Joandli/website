{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@layouts": ["src/layouts"],
      "@layouts/*": ["src/layouts/*"],
      "@assets": ["src/assets"],
      "@assets/*": ["src/assets/*"],
      "@util": ["src/util"],
      "@util/*": ["src/util/*"],
      "@types": ["src/types"],
      "@types/*": ["src/types/*"]
    }
  }
}
